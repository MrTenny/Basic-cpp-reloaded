#include <iostream>

using namespace std;

int wiek;

int main()
{
    cout << "How old are you?: ";
    cin >> wiek;

    if(wiek<18)
    {
        cout << "You are not an adult" << endl;
    }
    else if((wiek>=18)&&(wiek<35))
    {
        cout << "You are an adult but you can't be president." << endl;
    }
    else
    {
        cout << "You are an adult and you can be president." << endl;
    }
    return 55;
}
